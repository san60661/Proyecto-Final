<h2>Residencias</h2>

<table class="table">
	<thead>
		<tr>
			<th width="80%"> Nombre de la esidencia </th>
			<th width="20%"> Lugar </th>
			<th colspan="2"></th>
		</tr>
	</thead>

<tbody>
	<% @residences.each do |residence| %>
	<tr>
		<td><%=residence.name %> </td>
		<td><%=residence.description %></td>
		<td><%=link_to 'Editar',edit_residence_path(@residences)%></td>
		<td><%=link_to 'eliminar', @residence, method: :destroy%></td> 
	</tr>
	<% end %>
</tbody>
</table>
